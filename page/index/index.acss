.login-content {
  padding: 0rpx;
  background-color: #fff;
  width: 100%;
  height: 100%;
  position: fixed
}
.login-bg{
  width: 100%;
}
button.login-btn{
  background-color: #4768F3;
  color: #FFF;
  border-radius: 10rpx;
}
.login-form-container {
  width: 100%;
}
.form-content {
  padding: 0 40rpx;
}
.login-form {
  margin-left: 40rpx;
  margin-right: 40rpx;
}
.line{
  margin: 4rpx auto;
  width: 100%;
  height: 4rpx;
  background-color: #D6D6D6;
}
.form-section {
  width: 100%;
  position: relative;
  height: 120rpx;

}

.login-label {
    position: absolute;
    z-index: 3;
    top: 0;
    left: 40rpx;
    margin-top: 29rpx;
    width: 62rpx;
    height: 62rpx;
    /* border-left: 1px solid #bdbdbd; */
}
.name-label {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.pwd-label{
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
input {
  padding-left: 120rpx;
  padding-top: 29rpx;
  height: 68rpx;
}
form.login-form .clear-label{
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  position: absolute;
  z-index: 3;
  top: 0;
  right: 20rpx;
  margin-top: 29rpx;
  width: 62rpx;
  height: 62rpx;
}

form.login-form .not-see-pwd-label {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  position: absolute;
  z-index: 3;
  top: 0;
  right: 20rpx;
  margin-top: 29rpx;
  width: 62rpx;
  height: 62rpx;
}

form.login-form .see-pwd-label {
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  position: absolute;
  z-index: 3;
  top: 0;
  right: 20rpx;
  margin-top: 29rpx;
  width: 62rpx;
  height: 62rpx;
}

.login-footer{
  width: 100%;
  height: 100%;
  background-color: #fff;
}